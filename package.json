{
  "name": "archive-player",
  "version": "2.0.0",
  "description": "Archive Player - Angular + TypeScript optimized video player with Firebase",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "preview": "ng build && npx http-server -c-1 -o -p 8080 ./dist/archive-player",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "deploy": "npm run build && vercel --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "firebase": "^10.7.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0",
    "video.js": "^8.23.6",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@types/node": "^20.10.0",
    "@types/video.js": "^7.3.50",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
